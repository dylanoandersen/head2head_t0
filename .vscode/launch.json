{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Django Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/H2H/manage.py",
            "args": ["runserver"],
            "django": true,
            "cwd": "${workspaceFolder}/H2H",
